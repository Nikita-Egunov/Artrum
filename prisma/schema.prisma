datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") // Подтянется из .env
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id           Int     @id @default(autoincrement())
  email        String  @unique
  password     String
  refreshToken String
  moneyWasted  Int     @default(0)
  name         String?
}
